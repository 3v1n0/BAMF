<?xml version="1.0" encoding="UTF-8" ?>

<node name="/org/ayatana/bamf">

	<interface name="org.ayatana.bamf.tab">

		<signal name="UriChanged">
			<arg type="s" name="old" direction="out" />
			<arg type="s" name="new" direction="out" />
		</signal>

		<method name="ShowTab">
			<annotation name="org.freedesktop.DBus.GLib.CSymbol" value="bamf_tab_show" />
		</method>

		<method name="ParentXid">
			<annotation name="org.freedesktop.DBus.GLib.CSymbol" value="bamf_tab_parent_xid" />
			<arg type="u" name="xid" direction="out">
				<annotation name="org.freedesktop.DBus.GLib.ReturnVal" value=""/>
			</arg>
		</method>

		<method name="CurrentUri">
			<annotation name="org.freedesktop.DBus.GLib.CSymbol" value="bamf_tab_current_uri" />
			<arg type="s" name="uri" direction="out">
				<annotation name="org.freedesktop.DBus.GLib.ReturnVal" value=""/>
			</arg>
		</method>

		<method name="Preview">
			<annotation name="org.freedesktop.DBus.GLib.CSymbol" value="bamf_tab_get_preview" />
			<arg type="ay" name="data" direction="out">
				<annotation name="org.freedesktop.DBus.GLib.ReturnVal" value=""/>
			</arg>
		</method>
		
	</interface>
	
</node>
